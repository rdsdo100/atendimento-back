   },
   "scripts": {
      "build": "tsc",
      "dev": "ts-node-dev  src/server.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "start": "build",
      "start:local": "npm run build && build ",
