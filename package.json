      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^4.1.0",
      "@typescript-eslint/parser": "^4.1.0",
      "eslint": "^7.8.1",
      "prettier": "^2.1.1",
      "ts-node-dev": "^1.0.0-pre.51",
      "typescript": "^3.9.7"
   },
   "dependencies": {
      "celebrate": "^12.1.2",
      "class-validator": "^0.12.2",
